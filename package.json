{
  "name": "map-factory",
  "version": "1.0.1",
  "description": "Object mapping tool",
  "main": "./dist/lib/index.js",
  "scripts": {
    "babel": "babel ./lib/ --out-dir ./dist/lib/ --source-maps && babel ./test/ --out-dir ./dist/test/ --source-maps",
    "prebuild": "npm run clean && npm run eslint",
    "build": "npm run babel",
    "clean": "rimraf dist",
    "precover": "npm run build",
    "cover": "istanbul cover ./node_modules/nodeunit/bin/nodeunit ./dist/test",
    "coveralls": "istanbul cover ./node_modules/nodeunit/bin/nodeunit ./dist/test && cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "eslint": "eslint 'lib/**/*.es6' 'test/**/*.es6'",
    "pretest": "npm run build",
    "test": "nodeunit ./dist/test/",
    "pretravis": "npm run build",
    "travis": "npm run coveralls",
    "tsc": "mkdir dist && tsc --pretty -p ./dist/test -d --outDir ./dist/test",
    "premajor": "npm run build",
    "major": "npm version major -m \"published to npm as v%s\" && git push --all && npm publish",
    "preminor": "npm run build",
    "minor": "npm version minor -m \"published to npm as v%s\" && git push --all && npm publish",
    "prepatch": "npm run build",
    "patch": "npm version patch -m \"published to npm as v%s\" && git push --all && npm publish"
  },
  "bugs": {
    "url": "https://github.com/midknight41/map-factory/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/midknight41/map-factory.git"
  },
  "keywords": [
    "object",
    "mapping",
    "mapper",
    "fluent",
    "transform"
  ],
  "author": "Chad Macey <chad_macey@hotmail.com>",
  "contributors": [
    "Chad Macey <chad_macey@hotmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "object-mapper": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015-node4": "^2.1.0",
    "coveralls": "^2.11.9",
    "eslint": "^3.1.0",
    "gently": "^0.10.0",
    "istanbul": "^0.4.4",
    "jscoverage": "^0.6.0",
    "nodeunit": "^0.9.1",
    "rimraf": "^2.5.3",
    "typescript": "^1.8.10"
  },
  "files": [
    "dist"
  ]
}
